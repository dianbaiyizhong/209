	<bean id="configWarningLineService" class="com.qawaa.service.impl.ConfigWarningLineServiceImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="eventListService" class="com.qawaa.service.impl.EventListServiceImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="pointEventIntensityService" class="com.qawaa.service.impl.PointEventIntensityServiceImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="pointMultipleDailyService" class="com.qawaa.service.impl.PointMultipleDailyServiceImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="pointMultipleHourlyService" class="com.qawaa.service.impl.PointMultipleHourlyServiceImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="pointMultipleRealTimeService" class="com.qawaa.service.impl.PointMultipleRealTimeServiceImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="securityAuthorizeService" class="com.qawaa.service.impl.SecurityAuthorizeServiceImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="systemVersionService" class="com.qawaa.service.impl.SystemVersionServiceImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<!-- Action -->
</beans>


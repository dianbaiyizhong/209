<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-lazy-init="true" default-autowire="byName">

	<!-- DAO -->
	<bean id="categoryEventDao" class="com.qawaa.data.dao.impl.CategoryEventDaoImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="categoryAreaDao" class="com.qawaa.data.dao.impl.CategoryAreaDaoImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="categorySiteOneDao" class="com.qawaa.data.dao.impl.CategorySiteOneDaoImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="categorySiteTwoDao" class="com.qawaa.data.dao.impl.CategorySiteTwoDaoImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="configWarningLineDao" class="com.qawaa.data.dao.impl.ConfigWarningLineDaoImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="eventListDao" class="com.qawaa.data.dao.impl.EventListDaoImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="pointEventIntensityDao" class="com.qawaa.data.dao.impl.PointEventIntensityDaoImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="pointMultipleDailyDao" class="com.qawaa.data.dao.impl.PointMultipleDailyDaoImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="pointMultipleHourlyDao" class="com.qawaa.data.dao.impl.PointMultipleHourlyDaoImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="pointMultipleRealTimeDao" class="com.qawaa.data.dao.impl.PointMultipleRealTimeDaoImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="securityAuthorizeDao" class="com.qawaa.data.dao.impl.SecurityAuthorizeDaoImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="systemVersionDao" class="com.qawaa.data.dao.impl.SystemVersionDaoImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<!-- Service -->
	<bean id="categoryAreaService" class="com.qawaa.service.impl.CategoryAreaServiceImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="categoryEventService" class="com.qawaa.service.impl.CategoryEventServiceImpl" abstract="false" lazy-init="default" autowire="default"></bean>
	<bean id="categorySiteOneService" class="com.qawaa.service.impl.CategorySiteOneServiceImpl" abstract="false" lazy-init="default" autowire="default"></bean>
